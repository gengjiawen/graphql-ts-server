{
  "name": "graphql-ts-server",
  "version": "1.0.1",
  "description": "graphql-ts-server",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/index.ts",
    "build": "rimraf dist && cross-env NODE_ENV=development tsc",
    "test": "cross-env NODE_ENV=test jest --forceExit"
  },
  "dependencies": {
    "@koa/cors": "3.3.0",
    "@koa/router": "^10.1.1",
    "apollo-server-koa": "3.9.0",
    "bcryptjs": "2.4.3",
    "class-validator": "0.13.2",
    "cross-env": "7.0.3",
    "graphql": "15.8.0",
    "ioredis": "5.1.0",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "koa-jwt": "4.0.3",
    "nodemailer": "6.7.6",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1",
    "typeorm": "0.3.7",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "28.1.4",
    "@types/jsonwebtoken": "8.5.8",
    "@types/kcors": "2.2.6",
    "@types/koa__router": "^8.0.11",
    "@types/node": "18.0.0",
    "@types/nodemailer": "6.4.4",
    "@types/uuid": "8.3.4",
    "axios": "0.27.2",
    "jest": "28.1.2",
    "nodemon": "^2.0.18",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "4.7.4"
  },
  "jest": {
    "testEnvironment": "node",
    "globalSetup": "./src/jest/callSetup.js",
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
