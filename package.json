{
  "name": "graphql-ts-server",
  "version": "1.0.1",
  "description": "graphql-ts-server",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/index.ts",
    "build": "rimraf dist && cross-env NODE_ENV=development tsc",
    "test": "cross-env NODE_ENV=test jest --forceExit"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@koa/router": "^10.1.1",
    "apollo-server-koa": "2.25.0",
    "bcryptjs": "2.4.3",
    "class-validator": "0.12.2",
    "cross-env": "7.0.2",
    "graphql": "15.5.1",
    "ioredis": "4.17.3",
    "koa-compress": "^5.0.1",
    "koa-jwt": "4.0.0",
    "nodemailer": "6.6.1",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1",
    "typeorm": "0.2.28",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/ioredis": "4.17.3",
    "@types/jest": "26.0.13",
    "@types/jsonwebtoken": "8.5.0",
    "@types/kcors": "2.2.3",
    "@types/koa__router": "^8.0.3",
    "@types/node": "15.6.1",
    "@types/nodemailer": "6.4.0",
    "@types/uuid": "8.3.0",
    "axios": "0.21.4",
    "jest": "26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "26.4.3",
    "ts-node": "^9.1.0",
    "typescript": "4.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "globalSetup": "./src/jest/callSetup.js",
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
