{
  "name": "graphql-ts-server",
  "version": "0.0.1",
  "description": "graphql-ts-server",
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/jest": "23.3.1",
    "@types/node": "10.5.8",
    "@types/uuid": "3.4.3",
    "@types/yup": "0.24.7",
    "gql2ts": "1.8.2",
    "jest": "23.5.0",
    "nodemon": "1.18.3",
    "prettier": "1.14.2",
    "ts-jest": "23.1.3",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.14.0",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "@gql2ts/from-schema": "^1.8.2",
    "bcryptjs": "2.4.3",
    "cross-env": "5.2.0",
    "graphql": "0.13.2",
    "graphql-import": "0.6.0",
    "graphql-request": "1.8.2",
    "graphql-tools": "3.1.1",
    "graphql-yoga": "1.16.0",
    "pg": "7.4.3",
    "reflect-metadata": "0.1.12",
    "typeorm": "0.2.7",
    "yup": "0.26.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "gen-schema-types": "ts-node scripts/createTypes.ts"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
